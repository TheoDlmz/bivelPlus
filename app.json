    },
    "android": {
      "package": "com.theodlmz.bivelPlus",
      "permissions": [ "CAMERA", "LOCATION" ],
      "config":{
        "googleMaps":{
          "apiKey":"AIzaSyBPI2CVkhaUCfxvcxPDdYkVqXBfAerTm7Q"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
