{
  "expo": {
    "name": "bivelPlus",
    "slug": "bivelPlus",
    "version": "0.1.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor":"#e1e3e8",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e1e3e8"
    },
